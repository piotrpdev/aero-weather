{
	"productName": "aero-weather",
	"version": "0.0.0",
	"identifier": "dev.piotrp.aero-weather",
	"build": {
		"beforeDevCommand": "yarn dev",
		"beforeBuildCommand": "yarn build",
		"devUrl": "http://localhost:1420",
		"frontendDist": "../dist"
	},
	"app": {
		"withGlobalTauri": true,
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "tauri-app",
				"width": 600,
				"height": 400,
				"titleBarStyle": "Overlay",
				"hiddenTitle": true
			}
		]
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
