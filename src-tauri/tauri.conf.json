{
	"productName": "Aero Weather",
	"version": "1.0.2",
	"identifier": "dev.piotrp.aero-weather",
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devUrl": "http://localhost:1420",
		"frontendDist": "../dist"
	},
	"app": {
		"withGlobalTauri": true,
		"macOSPrivateApi": true,
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "Aero Weather",
				"width": 854,
				"height": 601,
				"titleBarStyle": "Overlay",
				"hiddenTitle": true,
				"transparent": true
			}
		]
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
